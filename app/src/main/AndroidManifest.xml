<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ms.sapientia.gaitrecognitionapp">

    <application
        android:name="ms.sapientia.ro.gaitrecognitionapp.service.App"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ms.sapientia.ro.gaitrecognitionapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="ms.sapientia.ro.gaitrecognitionapp.old.MyService"/>
        <service android:name="ms.sapientia.ro.gaitrecognitionapp.service.BackgroundService"/>


    </application>
</manifest>